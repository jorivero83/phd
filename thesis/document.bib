@Article{GPMPSANG,
AUTHOR = {Garc\'{\i}a-Pacheco, Francisco Javier and Moreno-Pulido, Soledad and Naranjo-Guerra, Enrique and S\'anchez-Alzola, Alberto},
TITLE = {Non-Linear Inner Structure of Topological Vector Spaces},
